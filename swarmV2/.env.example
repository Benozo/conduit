# SwarmV2 RAG Infrastructure Environment Configuration
# Copy this file to .env and customize as needed

# Docker Volume Directory (where data will be persisted)
DOCKER_VOLUME_DIRECTORY=./data

# Milvus Configuration
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_USER=
MILVUS_PASSWORD=

# Weaviate Configuration
WEAVIATE_HOST=localhost
WEAVIATE_PORT=8080
WEAVIATE_SCHEME=http
WEAVIATE_API_KEY=

# PostgreSQL with pgvector Configuration
PGVECTOR_HOST=localhost
PGVECTOR_PORT=5432
PGVECTOR_DATABASE=vectordb
PGVECTOR_USER=postgres
PGVECTOR_PASSWORD=password

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Ollama Configuration
OLLAMA_HOST=http://localhost:11434
OLLAMA_DEFAULT_MODEL=llama3.2

# LLM Provider API Keys
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
COHERE_API_KEY=your-cohere-api-key
HUGGINGFACE_API_KEY=your-huggingface-api-key

# Cloudflare Workers AI Configuration
CLOUDFLARE_ACCOUNT_ID=your-account-id
CLOUDFLARE_API_TOKEN=your-api-token
CLOUDFLARE_CUSTOM_URL=https://intent.moanalabs.com
CLOUDFLARE_CUSTOM_API_KEY=sk-test-admin-key-123

# SwarmV2 Configuration
SWARM_NAME=rag-swarm
SWARM_MAX_AGENTS=50
SWARM_LOG_LEVEL=info

# UI Access Information
# Milvus UI (Attu): http://localhost:3000
# Weaviate Console: http://localhost:8081
# pgAdmin: http://localhost:5050 (admin@swarmv2.com / admin123)
# Redis Commander: http://localhost:8082
# Ollama WebUI: http://localhost:8083
# MinIO Console: http://localhost:9001 (minioadmin / minioadmin)
